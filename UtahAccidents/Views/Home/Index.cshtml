@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="~/flavy/favicon-32x32.png">

    <title>Utah Accidents 31</title>

    <!-- Bootstrap core CSS -->
    <link href="~/lib/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Custom styles for this template -->
    <link href="~/lib/css/carousel.css" rel="stylesheet" />

</head>
<body>

    <header>
        <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
            <a class="navbar-brand" href="#">Carousel</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" asp-controller="Home" asp-action="Index">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="SummaryData">Data</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Calculator">Calculator</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Home" asp-action="Insights">Analytics</a>
                    </li>
                </ul>

            </div>
        </nav>
    </header>

    <main role="main">

        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="first-slide" src="https://w0.peakpx.com/wallpaper/372/799/HD-wallpaper-road-light-long-exposure-night-aerial-view-dark.jpg" alt="First slide">
                    <div class="container">
                        <div class="carousel-caption text-left">
                            <h1>Accidents Database.</h1>
                            <p>Browse information that has been tracked and recorded for every car-related incident in the state of Utah in 2016, 2017, and 2019.</p>
                            <p><a class="btn btn-lg btn-primary" asp-controller="Home" asp-action="SummaryData" role="button">Access The Information</a></p>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="second-slide" src="https://images.unsplash.com/photo-1449965408869-eaa3f722e40d?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxzZWFyY2h8Mnx8c2FmZSUyMGRyaXZpbmd8ZW58MHx8MHx8&w=1000&q=80" alt="Second slide">
                    <div class="container">
                        <div class="carousel-caption">
                            <h1>How Safe Are You?</h1>
                            <p>Try out our Crash Severity Model. Find out how likely you are to be severly injured in a crash given different factors you can choose.</p>
                            <p><a class="btn btn-lg btn-primary" asp-controller="Home" asp-action="ScorePredictor" role="button">Calculate Your Risk!</a></p>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <img class="third-slide" src="https://www.reardonagency.com/wp-content/uploads/2019/09/drive-dark.jpeg" alt="Third slide">
                    <div class="container">
                        <div class="carousel-caption text-right">
                            <h1>Analytics.</h1>
                            <p>Learn more about trends, patterns, and anomalies across all incidents. Let us help you stay safe.</p>
                            <p><a class="btn btn-lg btn-primary" asp-controller="Home" asp-action="Insights" role="button">Discover Insights</a></p>
                        </div>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
            <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>

        </div>


        <!-- Marketing messaging and featurettes
        ================================================== -->
        <!-- Wrap the rest of the page in another container to center all the content. -->

        <div class="container marketing">

            <!-- Three columns of text below the carousel -->
            <div class="row">
                <div class="col-lg-4">
                    <img class="rounded-circle" src="https://w0.peakpx.com/wallpaper/464/826/HD-wallpaper-database-neon-icon-blue-background-neon-symbols-database-neon-icons-database-sign-computer-signs-database-icon-computer-icons.jpg" alt="Generic placeholder image" width="140" height="140">
                    <h2>Data</h2>
                    <p>View the accident information collected over a four year span from the Utah Department of Transportation. Includes data points on accident location, severity, and categorical attributes.</p>
                    <p><a class="btn btn-secondary" asp-controller="Home" asp-action="SummaryData" role="button">View Data &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
                <div class="col-lg-4">
                    <img class="rounded-circle" src="https://c4.wallpaperflare.com/wallpaper/854/214/410/accounting-black-budget-calculating-wallpaper-preview.jpg" alt="Generic placeholder image" width="140" height="140">
                    <h2>Prediction Analytics</h2>
                    <p>Look into the future and calculate the predicted severity of a potential accident. Customize your prediction with information about the location of a predicted incident and the drivers involved.</p>
                    <p><a class="btn btn-secondary" asp-controller="Home" asp-action="ScorePredictor" role="button">View Predictions &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
                <div class="col-lg-4">
                    <img class="rounded-circle" src="https://media.istockphoto.com/photos/analyst-working-with-business-analytics-and-data-management-system-on-picture-id1286642964?b=1&k=20&m=1286642964&s=170667a&w=0&h=doK0J0FhFqTF83bb5XggguZgbR-pF16ngrDcr7xG21o=" alt="Generic placeholder image" width="140" height="140">
                    <h2>Administrator Tools</h2>
                    <p>Authorized Users may create, edit, and delete incident records to ensure the highest standards of data quality. Following this link will prompt you to enter Login Credentials.</p>
                    <p><a class="btn btn-secondary" href="/admin" role="button">View Admin &raquo;</a></p>
                </div><!-- /.col-lg-4 -->
            </div><!-- /.row -->
